let skillGapYesSet = new Set<string>();
let skillGapNoSet = new Set<string>();

this.InputFilterData.forEach(item => {
  const email = item.AuthorEmail;
  const skillGap = item.SkillGap?.toString().trim().toLowerCase();

  if (skillGap === 'yes') {
    skillGapYesSet.add(email);
  } else if (skillGap === 'no' || skillGap === '' || skillGap === 'na' || skillGap === null) {
    skillGapNoSet.add(email);
  }
});

const skillGapYes = skillGapYesSet.size;
const skillGapNo = skillGapNoSet.size;
